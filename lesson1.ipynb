    "import random\n",
    "\n",
    "# 1. Başlangyç maglumatlar (Giriş we Dogry jogaplar)\n",
    "x_train = [1, 2, 3, 4, 5]\n",
    "y_true = [2, 4, 6, 8, 10]  # Biz neýronyň x * 2 etmegini isleýäris\n",
    "\n",
    "# 2. Neýronyň agramy (başda tötänleýin san)\n",
    "weight = random.uniform(0, 1)\n",
    "learning_rate = 0.01  # Öwreniş tizligi\n",
    "\n",
    "print(f\"Başlangyç agram (weight): {weight:.4f}\")\n",
    "\n",
    "# 3. Talylym (Training) prosessi\n",
    "for epoch in range(100):  # 100 gezek gaýtala we öwren\n",
    "    for i in range(len(x_train)):\n",
    "        x = x_train[i]\n",
    "        target = y_true[i]\n",
    "        \n",
    "        # Neýron çaklama edýär\n",
    "        prediction = x * weight\n",
    "        \n",
    "        # Ýalňyşlygy hasaplaýarys\n",
    "        error = target - prediction\n",
    "        \n",
    "        # Matematika: Agramy azajyk düzedýäris (Gradient Descent-iň iň ýönekeý görnüşi)\n",
    "        # Eger ýalňyşlyk uly bolsa, agramy köp üýtget, kiçi bolsa az üýtget.\n",
    "        weight += error * x * learning_rate\n",
    "\n",
    "print(f\"Öwrenişden soňky agram: {weight:.4f}\")\n",
    "\n",
    "# 4. Synag (Test)\n",
    "test_x = 10\n",
    "print(f\"Synag: {test_x} * {weight:.4f} = {test_x * weight:.2f}\")"
   ]
